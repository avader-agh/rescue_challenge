              <center>false</center>
            </submesh>
          </mesh>
        </geometry>
        <material>
          <diffuse>1.0 1.0 1.0</diffuse>
          <specular>1.0 1.0 1.0</specular>
          <pbr>
            <metal>
              <albedo_map>model://rcboat/materials/textures/Boat_Albedo.png</albedo_map>
              <normal_map>model://rcboat/materials/textures/Boat_Normal.png</normal_map>
              <metalness_map>model://rcboat/materials/textures/Boat_Metalness.png</metalness_map>
              <roughness_map>model://rcboat/materials/textures/Boat_Roughness.png</roughness_map>
            </metal>
