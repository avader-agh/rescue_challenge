<?xml version="1.0"?>
<sdf version="1.6">
  <model name="rcboat">
    <static>true</static>
    <link name="base_link">
      <collision name="collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <scale>1.0 1.0 1.0</scale>
            <uri>model://rcboat/meshes/rcboat.dae</uri>
          </mesh>
        </geometry>
      </collision>
      <visual name= "Propeller_visual">
